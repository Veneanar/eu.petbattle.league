<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/option.xsd">
	<import>
		<categories>
			<category name="petbattle"></category>
			<category name="petbattle.main">
				<parent>petbattle</parent>
			</category>
			<category name="petbattle.main.bnet">
				<parent>petbattle.main</parent>
			</category>			
			<category name="petbattle.api">
				<parent>petbattle</parent>
			</category>
			<category name="petbattle.api.bnet">
				<parent>petbattle.api</parent>
			</category>	
			<category name="petbattle.content">
				<parent>petbattle</parent>
			</category>						
		</categories>
		
		<option name="tornament_register_description">
				<categoryname>petbattle.content</categoryname>
				<optiontype>textI18n</optiontype>
				<supporti18n>1</supporti18n>
				<showorder>2</showorder>
				<defaultvalue>wcf.simplePage.description</defaultvalue>
		</option>
		<option name="tornament_register_content">
				<categoryname>petbattle.content</categoryname>
				<optiontype>textareaI18n</optiontype>
				<supporti18n>1</supporti18n>
				<showorder>3</showorder>
		</option>

		
		<options>
      		<option name="petbattle_show_colored_classes">
				<categoryname>petbattle.main</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
      		<option name="petbattle_token_active">
				<categoryname>petbattle.main</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>			
			<option name="petbattle_bnet_oauthclient">
				<categoryname>petbattle.main</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>hesu4m777htfnqnxsz2vz2n9sem25zd6</defaultvalue>
			</option>
      <option name="petbattle_bnet_oauthsecret">
        <categoryname>petbattle.main</categoryname>
        <optiontype>text</optiontype>
        <defaultvalue>cR5XXzj2hVQcCxgypchhbHzjuEcPaJPY</defaultvalue>
      </option>
      <option name="petbattle_bnet_host">
        <categoryname>petbattle.main.bnet</categoryname>
        <optiontype>select</optiontype>
        <selectoptions>
          <![CDATA[eu.battle.net:eu.battle.net
us.battle.net:us.battle.net
kr.battle.net:kr.battle.net
tw.battle.net:tw.battle.net]]>
        </selectoptions>
        <defaultvalue><![CDATA[eu.api.battle.net]]></defaultvalue>
      </option>
  
      		<option name="petbattle_bnet_api_active">
				<categoryname>petbattle.api.bnet</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="petbattle_bnet_api_saveasset">
				<categoryname>petbattle.api.bnet</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>				
			<option name="petbattle_bnet_api_key">
				<categoryname>petbattle.api.bnet</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>hesu4m777htfnqnxsz2vz2n9sem25zd6</defaultvalue>
			</option>					
			<option name="petbattle_bnet_api_region">
				<categoryname>petbattle.api.bnet</categoryname>
				<optiontype>select</optiontype>
				<selectoptions><![CDATA[EU:EU
US:US
KR:KR
TW:TW]]></selectoptions>
				<defaultvalue><![CDATA[EU]]></defaultvalue>
			</option>
      <option name="petbattle_bnet_api_host">
        <categoryname>petbattle.api.bnet</categoryname>
        <optiontype>select</optiontype>
        <selectoptions>
          <![CDATA[eu.api.battle.net:eu.api.battle.net
us.api.battle.net:us.api.battle.net
kr.api.battle.net:kr.api.battle.net
tw.api.battle.net:tw.api.battle.net]]>
        </selectoptions>
        <defaultvalue><![CDATA[eu.api.battle.net]]></defaultvalue>
      </option>
      <option name="petbattle_bnet_api_language">
				<categoryname>petbattle.api.bnet</categoryname>
				<optiontype>select</optiontype>
				<selectoptions><![CDATA[de_DE:de_DE
en_GB:en_GB				
es_ES:es_ES
fr_FR:fr_FR
ru_RU:ru_RU
ko_KR:ko_KR
zh_TW:zh_TW
pt_BR:pt_BR
es_MX:es_MX]]></selectoptions>
				<defaultvalue><![CDATA[de_DE]]></defaultvalue>
			</option>	
		</options>
	</import>
</data>